@model CalorieTracker.Models.tbl_food_log
@{
    double consumedValue = (double)Model.food_log_quantity * ((double)Model.tbl_food.food_quantity * (double)Model.tbl_food.food_weight);
}
<div class="foodCard">
    @Html.ActionLink(linkText: "View More", actionName: "Details", controllerName: "Food", routeValues: new { id = Model.food_log_id }, htmlAttributes: null)
    @Model.tbl_user.user_first_name @Model.tbl_user.user_second_name consumed @consumedValue@Model.tbl_food.food_types of @Model.tbl_food.food_name
</div>