@model List<CalorieTracker.Models.ViewModels.UserNutrientRDAModel>
<div class="row">
    @for (int i = 0; i < Model.Count; i++)
    {
        if (i % 4 == 0)
        {
            @: </div><div class='row'>
        }
        <div class="col-md-3">
            @Html.Partial("PartialViews/_NutrientRDACard", Model[i])
        </div>
    }
</div>